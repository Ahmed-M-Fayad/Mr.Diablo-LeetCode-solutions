    "class Solution:\n",
    "    def maxProfit(self, prices: List[int]) -> int:\n",
    "        mn = float('inf')\n",
    "        mx = 0\n",
    "        for p in prices:\n",
    "            ans = 0\n",
    "            if mn > p:\n",
    "                mn = p\n",
    "            elif mx < p-mn:\n",
    "                mx = p - mn\n",
    "        return mx"
   ]
